{
  "name": "regenerative-minigrid-dashboard",
  "version": "1.6.0",
  "private": true,
  "main": "electron-main.js",
  "scripts": {
    "dev": "next dev --hostname 0.0.0.0",
    "build": "tsc",
    "start": "next start -H 0.0.0.0",
    "lint": "next lint",
    "docs": "typedoc",
    "export": "next build && next export",
    "electron:dev": "concurrently \"npm:dev\" \"wait-on http://localhost:3000 && electron .\"",
    "electron:package": "next build && electron-builder",
    "cap:sync": "npm run export && npx cap sync",
    "cap:android": "npx cap open android",
    "cap:ios": "npx cap open ios",
    "mobile:dev:android": "npm run export && npx cap run android -l --external",
    "mobile:dev:ios": "npm run export && npx cap run ios -l --external",
    "inject:RUSL": "cp config/config.RUSL.ts config/dataPoints.ts && cp config/config.RUSL.constants.ts config/constants.ts",
    "inject:Ranna": "cp config/config.Ranna.ts config/dataPoints.ts && cp config/config.Ranna.constants.ts config/constants.ts",
    "inject:Colombo": "cp config/config.Colombo.ts config/dataPoints.ts && cp config/config.Colombo.constants.ts config/constants.ts",
    "inject:default": "cp config/config.Colombo.ts config/dataPoints.ts && cp config/config.Colombo.constants.ts config/constants.ts",
    "inject": "npm run inject:RUSL && npm run inject:Ranna && npm run inject:Colombo && npm run inject:default",
    "generate:docs": "typedoc --ousld t docs/api --theme hierarchy --readme none --excludeExternals --excludePrivate --excludeProtected --hideGenerator --disableSources --tsconfig tsconfig.json --plugin typedoc-plugin-markdown --plugin typedoc-theme-hierarchy",
    "vercel-build": "next build",
    "merge:prod": "bash scripts/merge_to_production.sh"
  },
  "dependencies": {
    "@capacitor/core": "^7.3.0",
    "@capacitor/preferences": "^7.0.1",
    "@google/genai": "^1.4.0",
    "@google/generative-ai": "^0.24.1",
    "@hookform/resolvers": "^5.0.0",
    "@next/swc-wasm-nodejs": "13.5.10",
    "@radix-ui/react-accordion": "^1.2.3",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-aspect-ratio": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.3",
    "@radix-ui/react-checkbox": "^1.1.4",
    "@radix-ui/react-collapsible": "^1.1.3",
    "@radix-ui/react-context-menu": "^2.2.6",
    "@radix-ui/react-dialog": "^1.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-hover-card": "^1.1.6",
    "@radix-ui/react-label": "^2.1.6",
    "@radix-ui/react-menubar": "^1.1.6",
    "@radix-ui/react-navigation-menu": "^1.2.5",
    "@radix-ui/react-popover": "^1.1.6",
    "@radix-ui/react-progress": "^1.1.2",
    "@radix-ui/react-radio-group": "^1.2.3",
    "@radix-ui/react-scroll-area": "^1.2.3",
    "@radix-ui/react-select": "^2.1.6",
    "@radix-ui/react-separator": "^1.1.2",
    "@radix-ui/react-slider": "^1.2.3",
    "@radix-ui/react-slot": "^1.2.2",
    "@radix-ui/react-switch": "^1.1.3",
    "@radix-ui/react-tabs": "^1.1.3",
    "@radix-ui/react-toast": "^1.2.6",
    "@radix-ui/react-toggle": "^1.1.2",
    "@radix-ui/react-toggle-group": "^1.1.2",
    "@radix-ui/react-tooltip": "^1.1.8",
    "@tailwindcss/postcss": "^4.1.0",
    "@types/node": "22.13.17",
    "@types/react": "19.0.12",
    "@types/react-dom": "19.0.4",
    "@xyflow/react": "^12.6.0",
    "antd": "^5.25.3",
    "badge-maker": "^4.1.0",
    "bufferutil": "^4.0.9",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "cors": "^2.8.5",
    "date-fns": "^4.1.0",
    "embla-carousel-react": "^8.5.2",
    "eslint": "^9.23.0",
    "eslint-config-next": "15.2.4",
    "file-saver": "^2.0.5",
    "framer-motion": "^12.6.3",
    "glob": "^11.0.1",
    "idb": "^8.0.2",
    "input-otp": "^1.4.2",
    "jszip": "^3.10.1",
    "lucide-react": "^0.486.0",
    "next": "^15.2.4",
    "next-themes": "^0.4.6",
    "node-opcua": "^2.150.0",
    "nodemailer": "^7.0.6",
    "papaparse": "^5.5.2",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.1",
    "react": "19.1.0",
    "react-countup": "^6.5.3",
    "react-day-picker": "^9.6.4",
    "react-dom": "19.1.0",
    "react-grid-layout": "^1.5.1",
    "react-hook-form": "^7.55.0",
    "react-resizable-panels": "^2.1.7",
    "react-virtualized": "^9.22.6",
    "react-virtualized-auto-sizer": "^1.0.26",
    "react-window": "^1.8.11",
    "reactflow": "^11.11.4",
    "recharts": "^2.15.1",
    "rimraf": "6.x",
    "sharp": "^0.34.2",
    "sonner": "^2.0.3",
    "tailwind-merge": "^3.1.0",
    "tailwindcss": "^4.1.0",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "5.8.2",
    "utf-8-validate": "^6.0.5",
    "uuid": "^9.0.1",
    "vaul": "^1.1.2",
    "ws": "^8.18.1",
    "xlsx": "^0.18.5",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@capacitor/android": "^7.3.0",
    "@capacitor/cli": "^7.3.0",
    "@capacitor/ios": "^7.3.0",
    "@types/file-saver": "^2.0.7",
    "@types/nodemailer": "^7.0.1",
    "@types/papaparse": "^5.3.16",
    "@types/react-grid-layout": "^1.3.5",
    "@types/react-virtualized": "^9.22.2",
    "@types/react-window": "^1.8.8",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.18.1",
    "concurrently": "^9.1.2",
    "electron": "^36.4.0",
    "electron-builder": "^26.0.12",
    "postcss": "^8.5.3",
    "postcss-loader": "^8.1.1",
    "ts-node": "^10.9.2",
    "typedoc": "^0.28.4",
    "typedoc-plugin-markdown": "^4.6.3",
    "typedoc-theme-hierarchy": "^6.0.0",
    "wait-on": "^8.0.3"
  },
  "description": "Offline Dashboard for Solar Mini Grid Management",
  "author": "Deeghayu Adhikari",
  "build": {
    "appId": "com.deegayu.avdashboard",
    "productName": "AV R&D",
    "directories": {
      "output": "dist_electron"
    },
    "files": [
      "electron-main.js",
      "package.json",
      ".next/**",
      "public/**",
      "!**/*.md",
      "!**/*.map"
    ],
    "asarUnpack": [
      "**/node_modules/next/**"
    ],
    "mac": {
      "target": "dmg"
    },
    "win": {
      "target": "nsis"
    },
    "linux": {
      "target": "AppImage"
    }
  }
}
